   "source": [
    "ser = serial.Serial(\"/dev/ttyUSB0\", 9600, timeout= 2, parity=serial.PARITY_NONE, bytesize=8, stopbits=1, xonxoff=0)\n",
    "ser.write(':SENS:FUNC: \"TEMP\"')\n",
    "ser.write(':ROUT:OPEN:ALL on\\r'.encode())\n",
    "\n",
    "ser.close()"
